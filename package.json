{
  "name": "resound-sound",
  "version": "1.0.0",
  "description": "A web audio sound library",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "start": "npm-run-all --parallel dev:server dev:reload",
    "dev:server": "cd src && http-server",
    "dev:reload": "cd src && browser-sync start --proxy 'localhost:8080' --files '**/*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calebjedhugo/resound-sound.git"
  },
  "keywords": [
    "web",
    "audio",
    "sound",
    "library"
  ],
  "author": "Caleb Hugo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/calebjedhugo/resound-sound/issues"
  },
  "homepage": "https://github.com/calebjedhugo/resound-sound#readme",
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5"
  }
}
